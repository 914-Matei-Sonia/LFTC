function p1(n):=
  i=2
  if(n==i):=
    return true
  i=i+1
  return p1(n)

function p2(a,b):=
  if(a==b):=
    return b
  return p2(b%a,a)

function p3(n):=
  sum=0
  for(i=0;i<n;i++):=
    sum=sum+i
  return sum

function p1err[n]:=
  i=2
  if(n==0 || n==1):
    return false
  if(n==i):=
    return true
